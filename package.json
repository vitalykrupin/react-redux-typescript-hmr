  "scripts": {
    "patch": "npm run test && webpack --env.type=patch --env.dist -p --config webpack.config.prod.js && npm run test:coverage",
    "minor": "npm run test && webpack --env.type=minor --env.dist -p --config webpack.config.prod.js && npm run test:coverage",
    "major": "npm run test && webpack --env.type=major --env.dist -p --config webpack.config.prod.js && npm run test:coverage",
    "build": "webpack -p --config webpack.config.prod.js --progress --colors --display-error-details",
    "dev": "webpack --env.dev --config webpack.config.dev.js --progress",
    "start": "webpack-dev-server --config webpack.config.dev.js --progress",
    "test": "jest --config jest.config.json --no-cache",
    "test:watch": "npm test -- --watchAll",
    "test:coverage": "jest --config jest.config.json --coverage"
  },
